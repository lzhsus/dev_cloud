/* pages/recordlist/recordlist.wxss */
page {
    background: #f6f6f6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}
#recordlist-page {
    display: flex;
    justify-content: flex-start;
    flex-direction: column;
    flex: 1;
}

.recordlist-page-container {
    width: 710rpx;height: 100%;margin:30rpx auto;
    display: flex;justify-content: flex-start;flex-direction: column;flex: 1;
}
.ul{
    padding: 20rpx 0rpx;box-sizing: border-box;
}
.ul .hint{
    font-size: 16px;color: #999;
    text-align: center;
    padding: 200rpx 0;box-sizing: border-box;
}
.ul .li{
    width: 710rpx;margin:12rpx auto;
    background: #fff;
    border-radius: 6px;overflow: hidden;
    padding: 12rpx 20rpx;box-sizing: border-box;
}
.ul .li .item{
    width: 100%;min-height: 60rpx;
    display: flex;justify-content: flex-start;align-items: center;
}
.ul .li .item .lable{
    font-size: 12px;color: #000;
    width: 200rpx;
}
.ul .li .item .vaule{
    font-size: 12px;color: rgb(39, 75, 238);
    width: 400rpx;
}
.ul .li .item .vaule.active{
    text-decoration:line-through;color: #999;
}